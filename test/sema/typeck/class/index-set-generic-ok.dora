//= ok
class Foo[T] { a: T, b: T }
impl[T] std::traits::IndexSet for Foo[T] {
    type Index = Int;
    type Item = T;
    fn set(index: Self::Index, value: Self::Item) {
        if index == 0 {
            self.a = value;
        } else {
            assert(index == 1);
            self.b = value;
        }
    }
}
fn f(x: Foo[Float64], value: Float64) {
    x(0) = value;
}
