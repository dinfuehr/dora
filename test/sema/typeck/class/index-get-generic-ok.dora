class Foo[T] { a: T, b: T }
impl[T] std::traits::IndexGet for Foo[T] {
    type Index = Int;
    type Item = T;
    fn get(index: Self::Index): Self::Item {
        if index == 0 {
            self.a
        } else {
            assert(index == 1);
            self.b
        }
    }
}
fn f(x: Foo[Float64]): Float64 {
    x(0)
}

