//= ok
trait Base[T] {
    fn base_method(): T;
}

trait Derived: Base[Int64] {
    fn derived_method(): Int64 {
        self.base_method() + 1
    }
}

class Foo

impl Base[Int64] for Foo {
    fn base_method(): Int64 { 42 }
}

impl Derived for Foo {}

fn test(foo: Foo) {
    foo.derived_method();
}
