    fun emitRex32ModRmAddressOptional(reg: Register, address: Address) {
        if address.rex != 0u8 || reg.needsRexBit() {
            let rex = 0x40i32 | address.rex.toInt32() | if reg.needsRexBit() { 0x04i32 } else { 0i32 };
            self.emitByte(rex.toUInt8());
