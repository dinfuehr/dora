struct XmmRegister(value: Int32)

impl XmmRegister {
    fun lowBits(): Int32 = self.value & 0b111i32;
    fun needsRexBit(): Bool = self.value > 7i32;
}

enum JumpDistance {
    Near,
    Far,
}

@pub class AssemblerX64 {
    buffer: AssemblerBuffer,
    jumps: Vec[(Int64, Label, JumpDistance)],
}

