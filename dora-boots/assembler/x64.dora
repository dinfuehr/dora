    fun emitAlu64Imm(reg: Register, imm: Immediate, modrm_reg: Int32, rax_opcode: UInt8) {
        assert(imm.isInt32());
        self.emitRex64RmField(reg);

        if imm.isInt8() {
            self.emitByte(0x83u8);
            self.emitModRm(0b11i32, modrm_reg, reg.lowBits());
            self.emitByte(imm.toUInt8());
        } else if reg == RAX {
            self.emitByte(rax_opcode);
            self.emitInt32(imm.toInt32());
        } else {
            self.emitByte(0x81u8);
            self.emitModRm(0b11i32, modrm_reg, reg.lowBits());
            self.emitInt32(imm.toInt32());
        }
    }

    fun emitAlu32Imm(reg: Register, imm: Immediate, modrm_reg: Int32, rax_opcode: UInt8) {
        assert(imm.isInt32());
        self.emitRex32RmFieldOptional(reg);

        if imm.isInt8() {
            self.emitByte(0x83u8);
            self.emitModRmOpcode(modrm_reg, reg);
            self.emitByte(imm.toUInt8());
        } else if reg == RAX {
            self.emitByte(rax_opcode);
            self.emitInt32(imm.toInt32());
        } else {
            self.emitByte(0x81u8);
            self.emitModRmOpcode(modrm_reg, reg);
            self.emitInt32(imm.toInt32());
        }
    }

