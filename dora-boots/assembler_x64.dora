fun assertAsmNop(code: MachineCode, length: Int64) {
    assert(length > 0L);

    var idx = 0L;
    while idx < length {
        assert(0x90Y == code.bytes(code.start + idx));
        idx = idx + 1L;
    }

    code.start = code.start + length;
}
