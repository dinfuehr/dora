    fun testlri(lhs: Register, imm: Immediate) {
        assert(imm.isInt32());

        if imm.isUInt8() {
            if lhs == RAX {
                self.emitByte(0xa8Y);
            } else if lhs.value < 4 {
                self.emitByte(0xf6Y);
                self.emitModRmOpcode(0b000, lhs);
            } else {
                self.emitRex(false, false, false, lhs.needsRexBit());
                self.emitByte(0xf6Y);
                self.emitModRmOpcode(0b000, lhs);
            }
            self.emitByte(imm.toUInt8());
        } else if lhs == RAX {
            self.emitByte(0xa9Y);
            self.emitInt32(imm.toInt32());
        } else {
            self.emitByte(0xf7Y);
            self.emitModRmOpcode(0b000, lhs);
            self.emitInt32(imm.toInt32());
        }
    }

