    assertAsm(buffer, 0x35Y, 0x80Y, 0Y, 0Y, 0Y);
    assertAsm(buffer, 0x41Y, 0x81Y, 0xf0Y, 0x7fY, 0xffY, 0xffY, 0xffY);
    assertAsm(buffer, 0x41Y, 0x81Y, 0xf7Y, 0x80Y, 0Y, 0Y, 0Y);
    assertAsmEnd(buffer);
}

