    fun emitVal1(v1: Int) {
        if isWide(v1) {
            self.emitWide();
            self.emitInt(v1);
        } else {
            self.emitByte(v1);
        }
    }

    fun emitVal2(v1: Int, v2: Int) {
        if isWide(v1) || isWide(v2) {
            self.emitWide();
            self.emitInt(v1);
            self.emitInt(v2);
        } else {
            self.emitByte(v1);
            self.emitByte(v2);
        }
    }

    fun emitVal3(v1: Int, v2: Int, v3: Int) {
        if isWide(v1) || isWide(v2) || isWide(v3) {
            self.emitWide();
            self.emitInt(v1);
            self.emitInt(v2);
            self.emitInt(v3);
        } else {
            self.emitByte(v1);
            self.emitByte(v2);
            self.emitByte(v3);
        }
    }

    fun emitVal4(v1: Int, v2: Int, v3: Int, v4: Int) {
        if isWide(v1) || isWide(v2) || isWide(v3) || isWide(v4) {
            self.emitWide();
            self.emitInt(v1);
            self.emitInt(v2);
            self.emitInt(v3);
            self.emitInt(v4);
        } else {
            self.emitByte(v1);
            self.emitByte(v2);
            self.emitByte(v3);
            self.emitByte(v4);
        }
    }

    fun emitVal5(v1: Int, v2: Int, v3: Int, v4: Int, v5: Int) {
        if isWide(v1) || isWide(v2) || isWide(v3) || isWide(v4) || isWide(v5) {
            self.emitWide();
            self.emitInt(v1);
            self.emitInt(v2);
            self.emitInt(v3);
            self.emitInt(v4);
            self.emitInt(v5);
        } else {
            self.emitByte(v1);
            self.emitByte(v2);
            self.emitByte(v3);
            self.emitByte(v4);
            self.emitByte(v5);
        }
    }

    fun emitWide() {
        self.emitByte(BC_WIDE);
    }

    fun emitByte(val: Int) {
        assert(val >= 0 && val <= 255);
        self.code.push(val.toByte());
    }

    fun emitInt(val: Int) {
        assert(val >= 0);
        self.emitByte(val & 0xFF);
        self.emitByte((val >>> 8) & 0xFF);
        self.emitByte((val >>> 16) & 0xFF);
        self.emitByte((val >>> 24) & 0xFF);
    }

    fun patchInt(idx: Int, val: Int) {
        assert(val >= 0);
        self.code.set(idx, (val & 0xFF).toByte());
        self.code.set(idx + 1, ((val >>> 8) & 0xFF).toByte());
        self.code.set(idx + 2 , ((val >>> 16) & 0xFF).toByte());
        self.code.set(idx + 3, ((val >>> 24) & 0xFF).toByte());
    }
}

fun isWide(val: Int) -> Bool {
    val > 255
}

class BytecodeLabel {
    var off: Int = -1;

    fun bindTo(offset: Int) -> BytecodeLabel {
        assert(offset >= 0);
        self.off = offset;
        self
    }

    fun isBound() -> Bool {
        self.off >= 0
    }

    fun offset() -> Int {
        assert(self.isBound());
        self.off
    }
}

class UnresolvedJump(let inst: Int, let target: Int, let label: BytecodeLabel)
