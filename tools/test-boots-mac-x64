#!/bin/sh

set -e

TARGET=x86_64-apple-darwin
cargo run -p dora --target $TARGET -- test --package boots dora-boots/boots.dora --test-boots --gc-verify tests/hello-world.dora
cargo run -p dora --target $TARGET -- --package boots dora-boots/boots.dora  --gc-verbose --emit-compiler --bootstrap-compiler tests/boots/hello.dora
ruby tools/tester.rb  --target $TARGET tests/boots
