open class Error(msg: String): Throwable(msg) {
  override fun printStackTrace() {
    if super.getMessage() !== nil {
      println("Error: " + self.getMessage());
    } else {
      println("Error");
    }

    let x = self.getStackTrace();

    var i = 0;

    while i < x.length() {
      println(i.toString() + ": " + x[i].toString());
      i = i + 1;
    }
  }
}