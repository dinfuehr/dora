    let result = List[T]::ofCapacity(len);
    var idx = 0;
    while idx < len {
      let option = self(idx);
      if (option.isSome()) {
        result.push(option.getOrPanic());
      }
      idx = idx + 1;
    }
    return result;
  }
}

