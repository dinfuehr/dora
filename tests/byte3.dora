//= cannon

fun main() {
    let ba = arrayEmpty[Byte]();
    assert(ba.length() == 0);

    let ba = arrayFill[Byte](10, 2Y);
    assert(ba.length() == 10);

    ba(0) = 10Y;
    ba(1) = 11Y;
    ba(2) = 12Y;

    assert(10Y == ba(0));
    assert(11Y == ba(1));
    assert(12Y == ba(2));
    assert(2Y == ba(9));
}