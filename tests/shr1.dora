fun main() {
  assert(8I == 8I >> 0I);
  assert(4I == 8I >> 1I);
  assert(2I == 8I >> 2I);
  assert(1I == 8I >> 3I);
  assert(0I == 8I >> 4I);

  assert(-8I == -8I >> 0I);
  assert(-4I == -8I >> 1I);
  assert(-2I == -8I >> 2I);
  assert(-1I == -8I >> 3I);
  assert(-1I == -8I >> 4I);

  assert(8I == 8I >>> 0I);
  assert(4I == 8I >>> 1I);
  assert(2I == 8I >>> 2I);
  assert(1I == 8I >>> 3I);
  assert(0I == 8I >>> 4I);

  assert(-8I == -8I >>> 0I);
  assert(2147483644I == -8I >>> 1I);
  assert(1073741822I == -8I >>> 2I);
  assert(536870911I == -8I >>> 3I);
  assert(268435455I == -8I >>> 4I);
}
