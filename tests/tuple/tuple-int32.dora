fun main() {
    assert(tpl1() == 1);
    assert(tpl2() == 2);
    assert(tpl3() == 3);
    assert(tplsum() == 6);
}

fun tpl1(): Int32 {
    let x = (1, 2, 3);
    x.0
}

fun tpl2(): Int32 {
    let x = (1, 2, 3);
    x.1
}

fun tpl3(): Int32 {
    let x = (1, 2, 3);
    x.2
}

fun tplsum(): Int32 {
    let x = (1, 2, 3);
    x.0 + x.1 + x.2
}
