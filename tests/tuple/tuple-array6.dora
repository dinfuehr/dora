//= vm-args "--gc=copy --gc-stress"

class Foo (value: Int32)

fn main() {
    let x = Array[(Int32, Foo)]::fill(1024i64, (0i32, Foo(0i32)));
    for i in std::range(0, x.size()) {
        x(i) = (i.toInt32(), Foo(i.toInt32()));
    }
    std::forceCollect();
    for i in std::range(0, x.size()) {
        assert(x(i).0 == i.toInt32() && x(i).0 == x(i).1.value);
    }
}
