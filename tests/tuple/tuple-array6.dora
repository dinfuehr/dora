//= vm-args "--gc=copy --disable-tlab --gc-stress"

class Foo (let value: Int32)

fun main() {
    let x = Array[(Int32, Foo)]::fill(1024L, (0I, Foo(0I)));
    for i in std::range(0I, x.size().toInt32()) {
        x(i.toInt64()) = (i, Foo(i));
    }
    std::forceCollect();
    for i in std::range(0I, x.size().toInt32()) {
        assert(x(i.toInt64()).0 == i && x(i.toInt64()).0 == x(i.toInt64()).1.value);
    }
}
