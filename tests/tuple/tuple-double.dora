fn main() {
    assert(tpl1() == 1.0);
    assert(tpl2() == 2.0);
    assert(tpl3() == 3.0);
    assert(tplsum() == 6.0);
}

fn tpl1(): Float64 {
    let x = (1.0, 2.0, 3.0);
    x.0
}

fn tpl2(): Float64 {
    let x = (1.0, 2.0, 3.0);
    x.1
}

fn tpl3(): Float64 {
    let x = (1.0, 2.0, 3.0);
    x.2
}

fn tplsum(): Float64 {
    let x = (1.0, 2.0, 3.0);
    x.0 + x.1 + x.2
}
