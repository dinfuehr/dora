var x: (String, Bool) = ("", false);
var y: (String, Bool) = ("", false);
var tmp: (String, Bool) = ("", false);

fun main() {
    x = ("my string", true);
    y = ("another string", false);
    tmp = x;

    x = y;
    y = tmp;

    assert(y.0 == "my string");
    assert(y.1);

    assert(x.0 == "another string");
    assert(!x.1);
}
