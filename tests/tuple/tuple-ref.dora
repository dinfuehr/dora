//= vm-args "--gc=copy --disable-tlab --gc-stress"

class Foo(let value: Int32) {
    forceCollect();
}

fun main() {
    assert(tpl1() == 1);
    assert(tpl2() == 2);
    assert(tpl3() == 3);
    assert(tplsum() == 6);
}

fun tpl1() -> Int32 {
    let x = (Foo(1), Foo(2), Foo(3));
    forceCollect();
    x.0.value
}

fun tpl2() -> Int32 {
    let x = (Foo(1), Foo(2), Foo(3));
    forceCollect();
    x.1.value
}

fun tpl3() -> Int32 {
    let x = (Foo(1), Foo(2), Foo(3));
    forceCollect();
    x.2.value
}

fun tplsum() -> Int32 {
    let x = (Foo(1), Foo(2), Foo(3));
    forceCollect();
    x.0.value + x.1.value + x.2.value
}
