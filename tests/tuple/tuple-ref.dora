//= vm-args "--gc=copy --disable-tlab --gc-stress"

class Foo(let value: Int32) {
    forceCollect();
}

fun main() {
    assert(tpl1() == 1I);
    assert(tpl2() == 2I);
    assert(tpl3() == 3I);
    assert(tplsum() == 6I);
}

fun tpl1() -> Int32 {
    let x = (Foo(1I), Foo(2I), Foo(3I));
    forceCollect();
    x.0.value
}

fun tpl2() -> Int32 {
    let x = (Foo(1I), Foo(2I), Foo(3I));
    forceCollect();
    x.1.value
}

fun tpl3() -> Int32 {
    let x = (Foo(1I), Foo(2I), Foo(3I));
    forceCollect();
    x.2.value
}

fun tplsum() -> Int32 {
    let x = (Foo(1I), Foo(2I), Foo(3I));
    forceCollect();
    x.0.value + x.1.value + x.2.value
}