fun main() {
  assert(!isl7(L1()));
  assert(isl7(L7()));
  assert(!isl7(LX()));
  assert(!isl7(nil));
}

fun isl7(a: L1) -> bool {
  return a is L7;
}

open class L1 {}
open class L2: L1 {}
open class L3: L2 {}
open class L4: L3 {}
open class L5: L4 {}
open class L6: L5 {}
class L7: L6 {}
class LX: L6 {}
