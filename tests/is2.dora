fun main() {
  assert(!isl7(L1()));
  assert(!isl7(L2()));
  assert(!isl7(L3()));
  assert(!isl7(L4()));
  assert(!isl7(L5()));
  assert(!isl7(L6()));
  assert(isl7(L7()));

  assert(!isl7(LX()));
  assert(!isl7(nil));
}

fun isl7(a: L1): Bool {
  return a is L7;
}

@open class L1 {}
@open class L2 extends L1 {}
@open class L3 extends L2 {}
@open class L4 extends L3 {}
@open class L5 extends L4 {}
@open class L6 extends L5 {}
class L7 extends L6 {}
class LX extends L6 {}
