//= vm-args "--gc-verify --gc=copy"

enum Foo { A(Int32, String, Float32), B }
class Bar(let foo: Foo)

fun main() {
    let object = createObject();
    std::forceCollect();
    match object.foo {
        Foo::A(a, b, c) => {
            assert(a == 112I);
            assert(b == "abc");
            assert(c == 17.5F);
        },
        Foo::B => unreachable[()](),
    }
}

fun createObject(): Bar {
    Bar(Foo::A(112I, "abc", 17.5F))
}
