//= vm-args "--gc-verify --gc=copy"

enum Foo { A(Int32, String, Float32), B }

fun main() {
    let array = createArray();
    std::forceCollect();
    match array(0L) {
        Foo::A(a, b, c) => {
            assert(a == 112I);
            assert(b == "abc");
            assert(c == 17.5F);
        },
        Foo::B => unreachable[()](),
    }
}

fun createArray(): Array[Foo] {
    Array[Foo](Foo::A(112I, "abc", 17.5F))
}
