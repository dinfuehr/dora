//= vm-args "--gc-verify"

enum Foo { A(Int32, String, Float32), B }
class Bar(var foo: Foo)

fun main() {
    let object = Bar(Foo::B);
    std::forceCollect();
    updateObject(object);
    std::forceMinorCollect();
    match object.foo {
        Foo::A(a, b, c) => {
            assert(a == 112I);
            assert(b == "abc");
            assert(c == 17.5F);
        },
        Foo::B => unreachable[()](),
    }
}

fun updateObject(object: Bar) {
    object.foo = Foo::A(112I, "abc", 17.5F);
}
