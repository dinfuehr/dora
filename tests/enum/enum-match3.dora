fun main() {
    assert(value1(None[Int32]) == 0I);
    assert(value1(Some[Int32](5I)) == 5I);

    assert(value2(Some[Int32](42I)) == 42I);
    assert(value2(None[Int32]) == 0I);
}

fun value1(x: Option[Int32]): Int32 {
    match x {
        Some(a) => a,
        None => 0I
    }
}

fun value2(x: Option[Int32]): Int32 {
    match x {
        None => 0I,
        Some(a) => a
    }
}
