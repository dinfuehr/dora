fun main() {
  let x = Byte::minValue().toStringBinary();
  assert(x == "0");

  let x = Byte::maxValue().toStringBinary();
  assert(x == "11111111");

  let x = 12.toByte().toStringBinary();
  assert(x == "1100");

  let x = 67.toByte().toStringBinary();
  assert(x == "1000011");

  let x = 129.toByte().toStringBinary();
  assert(x == "10000001");

  let x = (-1).toByte().toStringBinary();
  assert(x == "11111111");

  let x = (-129).toByte().toStringBinary();
  assert(x == "1111111");


  let x = Byte::minValue().toStringHex();
  assert(x == "0");

  let x = Byte::maxValue().toStringHex();
  assert(x == "FF");

  let x = 12.toByte().toStringHex();
  assert(x == "C");

  let x = 67.toByte().toStringHex();
  assert(x == "43");

  let x = 129.toByte().toStringHex();
  assert(x == "81");

  let x = (-1).toByte().toStringHex();
  assert(x == "FF");

  let x = (-129).toByte().toStringHex();
  assert(x == "7F");
}
