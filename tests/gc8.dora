fun main() {
  let node = Node(10I, Some[Node](Node(9I, None[Node], None[Node])), Some[Node](Node(11I, None[Node], None[Node])));
  std::forceCollect();
  assert(node.lhs.getOrPanic().value == 9I);
  assert(node.value == 10I);
  assert(node.rhs.getOrPanic().value == 11I);
}

class Node(let value: Int32, let lhs: Option[Node], let rhs: Option[Node])
