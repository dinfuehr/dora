fn main() {
  let x = 1.0f32.neg();
  assert(x == -1.0f32);

  let x = 1.0f32.plus(2.0f32);
  assert(x == 3.0f32);

  let x = 1.0f32 + 2.0f32;
  assert(x == 3.0f32);

  let x = 5.0f32.minus(2.0f32);
  assert(x == 3.0f32);

  let x = 5.0f32 - 2.0f32;
  assert(x == 3.0f32);

  let x = 3.0f32.times(4.0f32);
  assert(x == 12.0f32);

  let x = 3.0f32 * 4.0f32;
  assert(x == 12.0f32);

  let x = 9.0f32.div(8.0f32);
  assert(x == 1.125f32);

  let x = 9.0f32 / 8.0f32;
  assert(x == 1.125f32);

  let x = (-12.0f32).asInt32().toStringBinary();
  assert(x == "11000001010000000000000000000000");
}
