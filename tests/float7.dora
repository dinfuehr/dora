fun main() {
    let array = Array[Float32]::empty();
    assert(0L == array.size());

    let array = Array[Float32]::fill(10L, 2F);
    assert(10L == array.size());
    assert(2F == array(0L));
    assert(2F == array(9L));

    let array = Array[Float64]::empty();
    assert(0L == array.size());

    let array = Array[Float64]::fill(12L, 3D);
    assert(12L == array.size());
    assert(3D == array(0L));
    assert(3D == array(11L));
}