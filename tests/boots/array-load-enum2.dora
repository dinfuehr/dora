//= boots

fn main() {
    let arr = Array[Foo]::fill(100, Foo::A);
    arr(47) = Foo::B;
    match f(arr, 46) {
        Foo::A => {}
        _ => unreachable[()](),
    }
    match f(arr, 47) {
        Foo::B => {}
        _ => unreachable[()](),
    }
    match f(arr, 48) {
        Foo::A => {}
        _ => unreachable[()](),
    }
}

enum Foo { A, B, C(Int64) }

@optimizeImmediately fn f(x: Array[Foo], idx: Int64): Foo { x(idx) }
