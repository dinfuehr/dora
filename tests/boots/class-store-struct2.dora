//= boots

fn main() {
    let foo = Foo(Bar(a = MyInt(1), b = MyInt(2)));
    std::forceCollect();

    f(foo, Bar(a = MyInt(3), b = MyInt(4)));
    std::forceCollect();

    assert(foo.bar.a.value == 3);
    assert(foo.bar.b.value == 4);
}

class Foo(bar: Bar)
struct Bar { a: MyInt, b: MyInt }
class MyInt(value: Int64)

@Optimize fn f(x: Foo, bar: Bar) { x.bar = bar; }
