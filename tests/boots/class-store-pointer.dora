//= boots

fn main() {
    let foo1 = Foo(Bar(0));
    let foo2 = Foo(Bar(0));

    std::forceCollect();

    f(foo1, Bar(12));
    assert(foo1.bar.val == 12);

    f(foo2, Bar(17));
    assert(foo2.bar.val == 17);

    std::forceCollect();
}

class Foo(bar: Bar)
class Bar(val: Int64)

@Optimize fn f(x: Foo, y: Bar) { x.bar = y; }
