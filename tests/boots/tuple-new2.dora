//= boots
//= vm-args --gc=copy --gc-stress

fn main() {
    let foo = getfoo();
    assert(foo.0.value == 1);
    assert(foo.1.b.value == 2);
    assert(foo.1.c.value == 3);
}

struct Foo(b: Value, c: Value)
class Value(value: Int64)

@Optimize fn getfoo(): (Value, Foo) {
    let a = Value(1);
    let b = Value(2);
    let c = Value(3);
    let foo = (a, Foo(b, c));
    std::forceCollect();
    foo
}
