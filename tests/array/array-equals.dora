import std::Assert;

fun main() {
  assert(Array[Int32]() == Array[Int32]());
  assert(Array[Int32]().equals(Array[Int32]()));
  assert(Array[Int32](1) == Array[Int32](1));
  assert(Array[Int32](1).equals(Array[Int32](1)));
  assert(Array[Int32](1, 2, 3) == Array[Int32](1, 2, 3));
  assert(Array[Int32](1, 2, 3).equals(Array[Int32](1, 2, 3)));

  assert(Array[Int32](1, 2, 3) != Array[Int32](2, 3, 4));
  assert(Array[Int32](1, 2, 3) != Array[Int32](1, 2, 3, 4));
}
