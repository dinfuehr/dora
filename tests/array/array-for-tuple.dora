fun main() {
    let array = Array[(Int32, String)]((1I, "abc"), (2I, "def"), (3I, "ghi"));
    for (idx, name) in array {
        if idx == 1I {
            assert(name == "abc");
        } else if idx == 2I {
            assert(name == "def");
        } else if idx == 3I {
            assert(name == "ghi");
        } else {
            unreachable[()]();
        }
    }
}
