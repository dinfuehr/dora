class Foo {
    let array: Array[Int32] = Array[Int32](1I, 2I, 3I);
}

fun main() {
    let foo = Foo();
    assert((foo.array)(0L) == 1I);
    assert((foo.array)(1L) == 2I);
    assert((foo.array)(2L) == 3I);
    foo.array(0L) = 100I;
    assert((foo.array)(0L) == 100I);
    assert((foo.array)(1L) == 2I);
    assert((foo.array)(2L) == 3I);
}