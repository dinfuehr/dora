fun main() {
  let x = Array::fill[Bool](3L, true);
  assert(std::arrayContains[Bool](x, true));

  let x = Array::fill[Int32](3L, 3);
  assert(std::arrayContains[Int32](x, 3));

  let x = Array::fill[Int64](3L, 3L);
  assert(std::arrayContains[Int64](x, 3L));

  let x = Array::fill[Float64](3L, 0.0/0.0);
  assert(std::arrayContains[Float64](x, 0.0/0.0));
}
