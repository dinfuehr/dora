fun main() {
    let array = Array::new[String](10L);
    assert(array(0L) == "");
    assert(array(0L) == array(9L));

    let array = Array::new[Float64](10L);
    assert(array(0L) == 0.0);
    assert(array(9L) == 0.0);

    let array = Array::new[Float32](10L);
    assert(array(0L) == 0.0F);
    assert(array(9L) == 0.0F);
}
