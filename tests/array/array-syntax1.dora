fun main() {
    let a = Array::ofSizeUnsafe[Int32](10L);
    assert(array_get(a, 0L) == 0);

    array_set(a, 0L, 7);
    assert(array_get(a, 0L) == 7);
}

fun array_set(a: Array[Int32], idx: Int64, value: Int32) {
    a(idx) = value;
}

fun array_get(a: Array[Int32], idx: Int64): Int32 {
    return a(idx);
}
