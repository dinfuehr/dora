fn main() {
    assert(8i32 % 2i32 == 0i32);
    assert(7i32 % 2i32 == 1i32);
    assert((-3i32) % 2i32 == -1i32);
    assert((-2i32) % 2i32 == 0i32);

    assert(-7i32 % -2i32 == -1i32);
    assert(-7i32 % 2i32 == -1i32);
    assert(7i32 % -2i32 == 1i32);
    assert(7i32 % 2i32 == 1i32);

    assert(-7i32 / -2i32 == 3i32);
    assert(-7i32 / 2i32 == -3i32);
    assert(7i32 / -2i32 == -3i32);
    assert(7i32 / 2i32 == 3i32);
}
