const i: Int32 = 1i32;
const l: Int64 = 2i64;
const yes: Bool = true;
const zero: UInt8 = 0u8;
const f: Float32 = 3.0f32;
const d: Float64 = 4.0;

fn main() {
    assert(i == 1i32);
    assert(l == 2i64);
    assert(yes == true);
    assert(zero == 0u8);
    assert(f == 3.0f32);
    assert(d == 4.0);
}
