struct Foo(a: Int64, b: Int64, c: Int64)

fn main() {
    assert(fst(Foo(1, 3, 7)) == 1);
    assert(mid(Foo(1, 3, 7)) == 3);
    assert(lst(Foo(1, 3, 7)) == 7);

    assert(fst(Foo(7, 4, 1)) == 7);
    assert(mid(Foo(7, 4, 1)) == 4);
    assert(lst(Foo(7, 4, 1)) == 1);
}

fn fst(x: Foo): Int64 {
    let Foo(a, ..) = x;
    a
}

fn mid(x: Foo): Int64 {
    let Foo(.., a, _) = x;
    a
}

fn lst(x: Foo): Int64 {
    let Foo(.., a) = x;
    a
}
