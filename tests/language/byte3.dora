fn main() {
    let ba = Array[UInt8]::new();
    assert(ba.size() == 0i64);

    let ba = Array[UInt8]::fill(10i64, 2u8);
    assert(ba.size() == 10i64);

    ba(0i64) = 10u8;
    ba(1i64) = 11u8;
    ba(2i64) = 12u8;

    assert(10u8 == ba(0i64));
    assert(11u8 == ba(1i64));
    assert(12u8 == ba(2i64));
    assert(2u8 == ba(9i64));
}