fun main() {
  let x = Float64::infinityNegative().roundUp();
  assert(x == Float64::infinityNegative());

  let x = (-2.5).roundUp();
  assert(x == -2.0);

  let x = (-1.5).roundUp();
  assert(x == -1.0);

  let x = (-2.5).roundUp();
  assert(x == -2.0);

  let x = (-1.5).roundUp();
  assert(x == -1.0);

  let x = (-1.23).roundUp();
  assert(x == -1.0);

  let x = (-1.0).roundUp();
  assert(x == -1.0);

  let x = (-0.0).roundUp();
  assert(x == -0.0);

  let x = 0.0.roundUp();
  assert(x == 0.0);

  let x = 1.0.roundUp();
  assert(x == 1.0);

  let x = 1.23.roundUp();
  assert(x == 2.0);

  let x = 1.5.roundUp();
  assert(x == 2.0);

  let x = 2.5.roundUp();
  assert(x == 3.0);

  let x = Float64::infinityPositive().roundUp();
  assert(x == Float64::infinityPositive());

  let x = Float64::notANumber().roundUp();
  assert(x.isNan());
}
