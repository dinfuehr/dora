fn main() {
    let array = Array[Float32]::new();
    assert(0i64 == array.size());

    let array = Array[Float32]::fill(10i64, 2f32);
    assert(10i64 == array.size());
    assert(2f32 == array(0i64));
    assert(2f32 == array(9i64));

    let array = Array[Float64]::new();
    assert(0i64 == array.size());

    let array = Array[Float64]::fill(12i64, 3f64);
    assert(12i64 == array.size());
    assert(3f64 == array(0i64));
    assert(3f64 == array(11i64));
}