fn main() {
    let a = 1f32 + 2f32;
    let b = 2f32 - 1f32;
    let c = 2f32 * 3f32;
    let d = 4f32 / 2f32;

    assert(a == 3f32);
    assert(b == 1f32);
    assert(c == 6f32);
    assert(d == 2f32);

    let a = 1f64 + 2f64;
    let b = 2f64 - 1f64;
    let c = 2f64 * 3f64;
    let d = 4f64 / 2f64;

    assert(a == 3f64);
    assert(b == 1f64);
    assert(c == 6f64);
    assert(d == 2f64);
}