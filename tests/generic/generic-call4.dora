//= stdout "world\n"

trait Foo {
    fn bar(): Int32;
}

class A[T: Foo](t: T)

impl[T: Foo] A[T] {
    fn bar(): Int32 {
        return self.t.bar();
    }
}

class X

impl Foo for X {
    fn bar(): Int32 {
        println("world");
        return 2i32;
    }
}

fn main() {
    assert(2i32 == A[X](X()).bar());
}
