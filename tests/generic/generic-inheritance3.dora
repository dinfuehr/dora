fun main() {
    assert(Bar[String]().value == 10);
    assert(Bar[Float32]().value == 10);

    let x = Baz[Float32]();
    assert(x.test() == 10);
    x.assignMe();
    assert(x.test() == 20);
}

@open @abstract class Foo[A](var value: A)
@open class Bar[A]: Foo[Int32](10)
class Baz[A]: Bar[A] {
    fun test(): Int32 { self.value }
    fun assignMe() { self.value = 20; }
}