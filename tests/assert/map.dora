import std::Assert;
import std::HashMap;

fun main() {
  let map = HashMap[Int32, String]();
  map.insert(1I, "a");
  map.insert(2I, "b");
  map.insert(3I, "c");
  map.insert(3I, "d");

  Assert::map[Int32, String](map)
    .contains(1I)
    .containsAll(1I, 2I, 3I)
    .hasSize(3L)
    .isNotEmpty();

  Assert::map[Int32, String](HashMap[Int32, String]())
    .hasSize(0L)
    .isEmpty();
}
