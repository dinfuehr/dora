import std::BitSet;
import std::HashMap;

fun main() {
    let map = HashMap[Int32, String]();
    map.insert(1I, "a");
    map.insert(10000I, "b");
    map.insert(7I, "c");

    check_iterator1(map);

    assert(map.get(1I).getOrPanic() == "a");
    assert(map.get(10000I).getOrPanic() == "b");
    assert(map.get(7I).getOrPanic() == "c");
    assert(map.get(2I).isNone());
    assert(map.size() == 3L);
    assert(map.isEmpty().not());

    assert(map.contains(1I));
    assert(map.contains(10000I));
    assert(map.contains(7I));
    assert(!map.contains(2I));

    map.remove(10000I);

    assert(map.get(1I).getOrPanic() == "a");
    assert(map.get(10000I).isNone());
    assert(map.get(7I).getOrPanic() == "c");
    assert(map.get(2I).isNone());
    assert(map.size() == 2L);
    assert(map.isEmpty().not());

    assert(map.contains(1I));
    assert(!map.contains(10000I));
    assert(map.contains(7I));

    check_iterator2(map);
}

fun check_iterator1(map: HashMap[Int32, String]) {
    let it = map.makeIterator();
    let set = BitSet(3L);

    while it.hasNext() {
        let key_and_value = it.next();
        let key = key_and_value.0;
        let value = key_and_value.1;

        if key == 1I {
            assert(value == "a");
            assert(!set.contains(0L));
            set.insert(0L);
        } else if key == 10000I {
            assert(value == "b");
            assert(!set.contains(1L));
            set.insert(1L);
        } else if key == 7I {
            assert(value == "c");
            assert(!set.contains(2L));
            set.insert(2L);
        } else {
            unreachable[()]();
        }
    }

    assert(set.contains(0L));
    assert(set.contains(1L));
    assert(set.contains(2L));
}

fun check_iterator2(map: HashMap[Int32, String]) {
    let it = map.makeIterator();
    let set = BitSet(3L);

    while it.hasNext() {
        let key_and_value = it.next();
        let key = key_and_value.0;
        let value = key_and_value.1;

        if key == 1I {
            assert(value == "a");
            assert(!set.contains(0L));
            set.insert(0L);
        } else if key == 7I {
            assert(value == "c");
            assert(!set.contains(1L));
            set.insert(1L);
        } else {
            unreachable[()]();
        }
    }

    assert(set.contains(0L));
    assert(set.contains(1L));
}