fun main() {
    let x = 1I.unaryPlus();
    assert(x == (+1I));

    let x = 1I.unaryMinus();
    assert(x == (-1I));

    let x = 1I.plus(2I);
    assert(x == 3I);

    let x = 1I + 2I;
    assert(x == 3I);

    let x = 5I.minus(2I);
    assert(x == 3I);

    let x = 5I - 2I;
    assert(x == 3I);

    let x = 3I.times(4I);
    assert(x == 12I);

    let x = 3I * 4I;
    assert(x == 12I);

    let x = 9I.div(8I);
    assert(x == 1I);

    let x = 9I / 8I;
    assert(x == 1I);

    let x = 10I.mod(8I);
    assert(x == 2I);

    let x = 10I % 8I;
    assert(x == 2I);

    let x = 1I.bitwiseOr(2I);
    assert(x == 3I);

    let x = 1I | 2I;
    assert(x == 3I);

    let x = 5I.bitwiseAnd(6I);
    assert(x == 4I);

    let x = 5I & 6I;
    assert(x == 4I);

    let x = 1I.bitwiseXor(3I);
    assert(x == 2I);

    let x = 1I ^ 3I;
    assert(x == 2I);

    let x = 1I.not();
    assert(x == (-2I));

    let x = !1I;
    assert(x == (-2I));

    assert(2I != 3I);
    assert(5I == 5I);
    assert(2I > 1I);
    assert(2I >= 1I);
    assert(3I < 4I);
    assert(3I <= 4I);
}
