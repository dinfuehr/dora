import std::HashMap;

fun main() {
    let map = HashMap[Int32, String]();
    assert(map.isEmpty());

    var i = 0;

    while i < 1000 {
        map.insert(i, i.toString());
        i = i + 1;
    }

    assert(map.size() == 1000L);
    assert(map.isEmpty().not());
    assert(map.capacity() == 2048L);
    assert(!map.contains(-1));
    assert(!map.contains(1000));

    i = 0;

    while i < 1000 {
        assert(map.get(i).getOrPanic() == i.toString());
        assert(map.contains(i));
        i = i + 1;
    }

    i = 900;

    while i < 1000 {
        map.remove(i);
        assert(!map.contains(i));
        i = i + 1;
    }

    assert(map.size() == 900L);
    assert(map.isEmpty().not());
    assert(map.capacity() == 2048L);

    i = 0;

    while i < 900 {
        assert(map.get(i).getOrPanic() == i.toString());
        assert(map.contains(i));
        i = i + 1;
    }

    i = 300;

    while i < 900 {
        map.remove(i);
        assert(!map.contains(i));
        i = i + 1;
    }

    assert(map.size() == 300L);
    assert(map.isEmpty().not());
    assert(map.capacity() == 1024L);
}
