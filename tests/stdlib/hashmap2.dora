fun main() {
    let map = HashMap[Int, String]();

    var i = 0;

    while i < 1000 {
        map.insert(i, i.toString());
        i = i + 1;
    }

    assert(map.len() == 1000);
    assert(map.capacity() == 2048);
    assert(!map.contains(-1));
    assert(!map.contains(1000));

    i = 0;

    while i < 1000 {
        assert(map.get(i).value == i.toString());
        assert(map.contains(i));
        i = i + 1;
    }

    i = 900;

    while i < 1000 {
        map.remove(i);
        assert(!map.contains(i));
        i = i + 1;
    }

    assert(map.len() == 900);
    assert(map.capacity() == 2048);

    i = 0;

    while i < 900 {
        assert(map.get(i).value == i.toString());
        assert(map.contains(i));
        i = i + 1;
    }

    i = 300;

    while i < 900 {
        map.remove(i);
        assert(!map.contains(i));
        i = i + 1;
    }

    assert(map.len() == 300);
    assert(map.capacity() == 1024);
}