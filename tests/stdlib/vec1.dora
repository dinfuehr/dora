fun main() {
    let vec = Vec[Int32]();
    assert(vec.size() == 0L);
    assert(vec.capacity() == 0L);

    vec.push(1I);
    vec.push(2I);
    vec.push(3I);
    assert(vec.size() == 3L);
    assert(vec.capacity() == 4L);

    vec.push(4I);
    assert(vec.size() == 4L);
    assert(vec.capacity() == 4L);

    vec.push(5I);
    assert(vec.size() == 5L);
    assert(vec.capacity() == 8L);
    assert(vec(0L) == 1I);
    assert(vec(1L) == 2I);
    assert(vec(2L) == 3I);
    assert(vec(3L) == 4I);
    assert(vec(4L) == 5I);

    vec(0L) = vec(0L) + 2I;
    vec(1L) = vec(0L) - 1I;

    assert(vec(0L) == 3I);
    assert(vec(1L) == 2I);

    vec.trimToLen();
    assert(vec.size() == 5L);
    assert(vec.capacity() == 5L);

    assert(vec(0L) == 3I);
    assert(vec(1L) == 2I);
    assert(vec(2L) == 3I);
    assert(vec(3L) == 4I);
    assert(vec(4L) == 5I);
}
