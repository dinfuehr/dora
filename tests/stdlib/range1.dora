fun main() {
    let x = std::range(0I, 5I).makeIterator();

    assert(x.hasNext());
    assert(x.next() == 0I);

    assert(x.hasNext());
    assert(x.next() == 1I);

    assert(x.hasNext());
    assert(x.next() == 2I);

    assert(x.hasNext());
    assert(x.next() == 3I);

    assert(x.hasNext());
    assert(x.next() == 4I);

    assert(!x.hasNext());
}
