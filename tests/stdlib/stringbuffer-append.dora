//= cannon

fun main() {
    let buf = StringBuffer::empty();
    buf.appendChar('a');
    buf.appendChar('b');
    buf.appendChar('c');
    assert(buf.length() == 3);
    assert(buf.toString() == "abc");

    buf.append("foo123");
    assert(buf.length() == 9);
    assert(buf.toString() == "abcfoo123");
}
