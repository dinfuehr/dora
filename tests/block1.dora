fun main() {
    t1();
    t2();
    t3();
}

fun g(): Int32 { return 1; }

fun t1() {
    let x: Int32 = { g() };
    assert(x == 1);
}

fun t2() {
    let x: Int32 = { g(); 1 };
    assert(x == 1);
}

fun t3() {
    let x: String = "x${1}y" + { forceCollect(); "end" };
    assert(x == "x1yend");
}
