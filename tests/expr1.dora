fun main() {
    assert((1I + 3I) == 4I);
    assert((-1I + 3I) == 2I);
    assert((0I + 2I) == 2I);
    assert((2I + 0I) == 2I);

    let x = -5I + 4I;
    assert(-1I == x);

    assert(+1I != -1I);
    assert(+2I == 2I);

    assert(!(-1I) == 0I);
    assert(!(-2I) == 1I);
    assert(-1I == !0I);

    assert(true);
    assert(!false);

    let x = true;
    assert(x);

    let x = false;
    assert(!x);

    assert(10I == 1I+(2I+(3I+4I)));
    assert(-1I == 1I-2I);
    assert(3I == 2I-(3I-4I));
    assert(-2I == 1I-(2I-(3I-4I)));
    assert(42I == 7I*(1I+(2I+3I)));

    assert(3I == 1I|2I);
    assert(1I == 1I&3I);
    assert(1I == 3I^2I);

    assert(6I == 2I*3I);
    assert(2I == 8I/3I);
    assert(0I == 9I%3I);

    assert(true == (true || true));
    assert(true == (true || false));
    assert(true == (false || true));
    assert(false == (false || false));

    assert(true == (true && true));
    assert(false == (true && false));
    assert(false == (false && true));
    assert(false == (false && false));
}
