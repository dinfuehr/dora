//= vm-args "--gc=swiper --gc-verify"

fun main() {
    let filler = Foo(101I);
    let a = Array[Foo]::fill(61L, filler);
    let b = Array[Foo]::fill(61L, filler);
    std::forceMinorCollect();
    std::forceMinorCollect();
    // a(5L) = Foo(2I);
    b(5L) = Foo(1I);
    std::forceMinorCollect();
    // assert(a(5).a == 2I);
    assert(b(5L).a == 1I);
}

class Foo(let a: Int32)
