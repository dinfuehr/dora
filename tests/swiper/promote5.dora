//= vm-args "--gc=swiper --gc-verify"

class Foo(let x: Int32)

fun main() {
    let x = Array::ofSizeUnsafe[Foo](64L);
    forceMinorCollect();
    assert(x(0L) === nil);
    assert(x(63L) === nil);

    x(0L) = Foo(1);
    forceMinorCollect();
    assert(x(0L).x == 1);
    assert(x(63L) === nil);

    x(63L) = Foo(2);
    forceMinorCollect();
    assert(x(0L).x == 1);
    assert(x(63L).x == 2);

    forceMinorCollect();
    assert(x(0L).x == 1);
    assert(x(63L).x == 2);
}
