//= vm-args "--gc=swiper --gc-verify"

fun main(): Unit {
  let default = Foo(42i32);
  let mut x = Array[Foo]::fill(128i64, default);
  std::forceMinorCollect();
  x(0i64) = Foo(1i32);
  x(127i64) = Foo(2i32);
  std::forceMinorCollect();
  assert(x(0i64).a == 1i32);
  assert(x(1i64) === default);
  assert(x(126i64) === default);
  assert(x(127i64).a == 2i32);
}

class Foo(a: Int32)
