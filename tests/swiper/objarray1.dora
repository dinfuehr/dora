//= vm-args "--gc=swiper --gc-verify"

fun main() {
  let x = Foo(100);
  let y = Foo(101);
  let z = Array::ofSizeUnsafe[Foo](512L);
  forceMinorCollect();
  forceMinorCollect();
  // write into old object x reference to young object
  z(0L) = Foo(2);
  z(511L) = Foo(3);
  forceMinorCollect();
  assert(z(0L).a == 2);
  assert(z(511L).a == 3);
}

class Foo(let a: Int32)
