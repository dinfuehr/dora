//= vm-args "--gc=swiper --gc-verify"

class Foo(x: Int32)

fun main(): Unit {
    let filler = Foo(0i32);
    let x = Array[Foo]::fill(16i64 * 1024i64, filler);
    std::forceCollect();
    assert(x(0i64) === filler);

    x(0i64) = Foo(1i32);
    std::forceMinorCollect();
    assert(x(0i64).x == 1i32);

    x(16i64 * 1024i64 - 1i64) = Foo(2i32);
    std::forceMinorCollect();
    assert(x(0i64).x == 1i32);
    assert(x(16i64 * 1024i64 - 1i64).x == 2i32);
}
