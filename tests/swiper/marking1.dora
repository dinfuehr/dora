//= ignore

fun main() {
    let arr = Vec[Foo]();

    var i = 0I;
    while i < 10I {
        arr.push(tree(9));
        i = i + 1I;
    }

    std::forceCollect();
    std::forceCollect();
    std::forceCollect();
    std::forceCollect();
    std::forceCollect();
}

fun tree(depth: Int32): Foo {
    if depth == 0I {
        return Foo(nil, nil, nil, nil, nil);
    }

    return Foo(
        tree(depth-1),
        tree(depth-1),
        tree(depth-1),
        tree(depth-1),
        tree(depth-1)
    );
}

class Foo(let a: Foo, let b: Foo, let c: Foo, let d: Foo, let e: Foo)
