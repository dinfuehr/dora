//= vm-args "--gc=swiper --gc-verify"

class Foo(let x: Int)

fun main() {
    let a = Array::<Long>(70);
    let x = CardSpanningObject();
    forceMinorCollect();
    assert(x.field0 === nil);
    assert(x.field64 === nil);

    x.field0 = Foo(1);
    forceMinorCollect();
    assert(x.field0.x == 1);
    assert(x.field64 === nil);

    forceMinorCollect();
    assert(x.field0.x == 1);
    assert(x.field64 === nil);
}

