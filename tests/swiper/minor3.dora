//= vm-args "--gc-parallel-minor --gc-worker=2 --gc-verify"

fun main() {
    var x = Vec[Array[Int32]]();
    var i = 0;

    while i < 100 {
        if i % 2 == 0 {
            x.push(Array[Int32](2L));
        } else {
            x.push(Array[Int32](4L));
        }
        x.push(Array[Int32](512L));
        x.push(Array[Int32](2048L));
        if i % 5 == 0 {
            forceMinorCollect();
        }
        i = i + 1;
    }
}

