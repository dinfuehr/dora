//= vm-args "--gc=swiper --gc-verify"

fun main(): Unit {
  let x = Array[Foo]::fill(2i64, Foo(42i32));
  std::forceMinorCollect();
  std::forceMinorCollect();
  // array in old generation references object in
  // young generation.
  x(0i64) = Foo(2i32);
  std::forceMinorCollect();
  assert(x(0i64).a == 2i32);
}

class Foo(a: Int32)
