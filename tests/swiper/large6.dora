//= vm-args "--gc=swiper --max-heap-size=32M --gc-verify"

const SIZE: Int64 = 100L;
const STEP: Int64 = 5L;
const REMOVED: Int64 = 20L;

fun main() {
    var x = Vec[Array[Int32]]();
    var i = 0L;

    while i < SIZE {
        x.push(Array::ofSizeUnsafe[Int32](32L * 1024L));
        i = i + 1L;
    }

    forceCollect();

    i = 0L;

    while i < SIZE {
        x(i) = nil;
        i = i + STEP;
    }

    forceCollect();

    i = 0L;
    var nils = 0L;

    while i < SIZE {
        if x(i) === nil {
            nils = nils + 1L;
        }

        i = i + 1L;
    }

    assert(nils == REMOVED);
}
