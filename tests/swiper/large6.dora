//= vm-args "--gc=swiper --max-heap-size=32M --gc-verify"

const SIZE: Int64 = 100L;
const STEP: Int64 = 5L;
const REMOVED: Int64 = 20L;

fun main() {
    var x = Vec[Array[Int32]]();
    var i = 0L;

    while i < SIZE {
        x.push(Array[Int32]::fill(32L * 1024L, 0I));
        i = i + 1L;
    }

    std::forceCollect();

    i = 0L;
    var empty = Array[Int32]::empty();

    while i < SIZE {
        x(i) = empty;
        i = i + STEP;
    }

    std::forceCollect();

    i = 0L;
    var empties = 0L;

    while i < SIZE {
        if x(i) === empty {
            empties = empties + 1L;
        }

        i = i + 1L;
    }

    assert(empties == REMOVED);
}
