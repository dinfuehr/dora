//= ignore

fn main() {
    let list = Array[Int64]::new(10, 20, 30, 40, 50);
    let lambdas = Vec[(): Int64]::new();

    for el in list {
        lambdas.push(||: Int64 { el });
    }

    // All lambdas close over the same variable, so last value wins.
    for lambda in lambdas {
        assert(lambda() == 50);
    }
}
