//= vm-args "--gc=swiper --gc-verify"

fn main() {
    let l0: (): Int64 = ||: Int64 { 0 };
    let l1: (): Int64 = ||: Int64 { 1 };
    std::forceCollect();
    let cls = MyClass(l0, l1);
    assert(cls.l0 === l0);
    assert(cls.l1 === l1);
    cls.l0 = l1;
    std::forceCollect();
    assert(cls.l0 === l1);
    assert(cls.l1 === l1);
    std::forceCollect();
}

