}

fn mytuple(lambda: ((): Int64, (): Int64)): ((): Int64, (): Int64) {
    lambda
}

struct MyStruct {
    l0: (): Int64,
    l1: (): Int64,
}

fn mystruct(l0: (): Int64, l1: (): Int64): MyStruct {
    MyStruct(l0, l1)
}

