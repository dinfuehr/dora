fun main(): Unit {
    let x = 257i32.toUInt8();
    assert(x == 1i32.toUInt8());

    let x = (-1i32).toUInt8();
    assert(x == 255i32.toUInt8());

    let x = 129u8.toInt32();
    assert(x == 129i32);

    let x = 129u8.toInt64();
    assert(x == 129i64);

    let x = 65u8.toChar();
    assert(x == 'A');
}
