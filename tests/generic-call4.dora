//= stdout "world\n"

trait Foo {
    fun bar(): Int32;
}

class A[T: Foo](let t: T) {
    fun bar(): Int32 {
        return self.t.bar();
    }
}

class X

impl Foo for X {
    fun bar(): Int32 {
        println("world");
        return 2I;
    }
}

fun main() {
    assert(2I == A[X](X()).bar());
}