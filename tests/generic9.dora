fun main() {
    let a = A[Int32](0I);
    consume(a.x);

    let b = A[String]("");
    consume2(b.x);
}

class A[T](default: T) {
    var x: Array[T] = Array[T]::fill(10L, default);
}

fun consume(x: Array[Int32]) {
    assert(x(9L) == 0I);
}

fun consume2(x: Array[String]) {
    assert(x.size() == 10L);
}
