fun main() {
    let a = A::<int>();
    consume(a.x);

    let b = A::<String>();
    consume2(b.x);
}

class A<T> {
    var x: Array<T> = nil;

    init() {
        self.x = Array::<T>(10);
    }
}

fun consume(x: Array<int>) {
    assert(x[9] == 0);
}

fun consume2(x: Array<String>) {
    assert(x[9] === nil);
}