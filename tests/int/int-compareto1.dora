fun main() {
    assert(2L.compareTo(4L) == -1I);
    assert(4L.compareTo(2L) == 1I);
    assert(4L.compareTo(4L) == 0I);

    assert((-2L).compareTo(-4L) == 1I);
    assert((-4L).compareTo(-2L) == -1I);
    assert((-4L).compareTo(-4L) == 0I);

    assert(2.compareTo(4) == -1I);
    assert(4.compareTo(2) == 1I);
    assert(4.compareTo(4) == 0I);

    assert((-2).compareTo(-4) == 1I);
    assert((-4).compareTo(-2) == -1I);
    assert((-4).compareTo(-4) == 0I);

    assert(2Y.compareTo(4Y) == -1I);
    assert(4Y.compareTo(2Y) == 1I);
    assert(4Y.compareTo(4Y) == 0I);

    assert('a'.compareTo('z') == -1I);
    assert('z'.compareTo('a') == 1I);
    assert('x'.compareTo('x') == 0I);
}
