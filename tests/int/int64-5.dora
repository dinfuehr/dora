fn main() {
    let ba = Array[Int64]::zero(0i64);
    assert(ba.size() == 0i64);

    let ba = Array[Int64]::fill(10i64, 2i64);
    assert(ba.size() == 10i64);

    ba(0i64) = 10i64;
    ba(1i64) = 11i64;
    ba(2i64) = 12i64;

    assert(10i64 == ba(0i64));
    assert(11i64 == ba(1i64));
    assert(12i64 == ba(2i64));
    assert(2i64 == ba(9i64));
}
