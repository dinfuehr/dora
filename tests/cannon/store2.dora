var glob_bool: Bool = false;
var glob_char: Char = 'x';
var glob_byte: UInt8 = 0Y;
var glob_int32: Int32 = 0I;
var glob_int64: Int64 = 0L;
var glob_float: Float32 = 0.0F;
var glob_double: Float64 = 0.0;
var glob_string: String = "";
var glob_obj: Foo = Foo();

fun main() {
    let obj = Foo();
    init_globs(true, '5', 5Y, 5I, 5L, 5F, 5D, "5", obj);
    
    assert(glob_bool == true);
    assert(glob_char == '5');
    assert(glob_byte == 5Y);
    assert(glob_int32 == 5I);
    assert(glob_int64 == 5L);
    assert(glob_float == 5F);
    assert(glob_double == 5D);
    assert(glob_string == "5");
    assert(glob_obj === obj);
}

fun init_globs(bool: Bool, char: Char, byte: UInt8, int32: Int32, int64: Int64, float32: Float32, float64: Float64, string: String, obj: Foo) {
    glob_bool = bool;
    glob_char = char;
    glob_byte = byte;
    glob_int32 = int32;
    glob_int64 = int64;
    glob_float = float32;
    glob_double = float64;
    glob_string = string;
    glob_obj = obj;  
}

class Foo()