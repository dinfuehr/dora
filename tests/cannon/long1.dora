fun main() {
    assert(not(20L) == -21L);

    assert(add(20L, 7L) == 27L);
    assert(sub(20L, 7L) == 13L);
    assert(mul(20L, 7L) == 140L);
    assert(div(20L, 7L) == 2L);
    
    assert(mod(20L, 7L) == 6L);

    assert(neg(20L) == -20L);
    assert(neg(-5L) == 5L);

    assert(or(20L, 5L) == 21L);
    assert(xor(20L, 5L) == 17L);
    assert(and(20L, 5L) == 4L);

    assert(shl(20L, 2) == 80L);
    assert(shr(20L, 2) == 5L);
    assert(sar(20L, 2) == 5L);

    assert(!eq(20L, 5L));
    assert(eq(5L, 5L));
    assert(!eq(5L, 20L));

    assert(ne(20L, 5L));
    assert(!ne(5L, 5L));
    assert(ne(5L, 20L));


    assert(!lt(20L, 5L));
    assert(!lt(5L, 5L));
    assert(lt(5L, 20L));

    assert(!le(20L, 5L));
    assert(le(5L, 5L));
    assert(le(5L, 20L));

    assert(ge(20L, 5L));
    assert(ge(5L, 5L));
    assert(!ge(5L, 20L));

    assert(gt(20L, 5L));
    assert(!gt(5L, 5L));
    assert(!gt(5L, 20L));
}

fun not(x: Int64): Int64 {
    return !x;
}

fun add(x: Int64, y: Int64): Int64 {
    return x+y;
}

fun sub(x: Int64, y: Int64): Int64 {
    return x-y;
}

fun mul(x: Int64, y: Int64): Int64 {
    return x*y;
}

fun div(x: Int64, y: Int64): Int64 {
    return x/y;
}

fun mod(y: Int64, z: Int64): Int64 {
    return y%z;
}

fun or(x: Int64, y: Int64): Int64 {
    return x | y;
}

fun xor(x: Int64, y: Int64): Int64 {
    return x ^ y;
}

fun and(x: Int64, y: Int64): Int64 {
    return x & y;
}

fun shl(x: Int64, y: Int32): Int64 {
    return x << y;
}

fun shr(x: Int64, y: Int32): Int64 {
    return x >>> y;
}

fun sar(x: Int64, y: Int32): Int64 {
    return x >> y;
}

fun eq(x: Int64, y: Int64): Bool {
    return x == y;
}

fun ne(x: Int64, y: Int64): Bool {
    return x != y;
}

fun lt(x: Int64, y: Int64): Bool {
    return x < y;
}

fun le(x: Int64, y: Int64): Bool {
    return x <= y;
}

fun ge(x: Int64, y: Int64): Bool {
    return x >= y;
}

fun gt(x: Int64, y: Int64): Bool {
    return x > y;
}

fun neg(x: Int64): Int64 {
    return -x;
}