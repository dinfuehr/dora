fun main() {
    assert(8I % 2I == 0I);
    assert(7I % 2I == 1I);
    assert((-3I) % 2I == -1I);
    assert((-2I) % 2I == 0I);

    assert(-7I % -2I == -1I);
    assert(-7I % 2I == -1I);
    assert(7I % -2I == 1I);
    assert(7I % 2I == 1I);

    assert(-7I / -2I == 3I);
    assert(-7I / 2I == -3I);
    assert(7I / -2I == -3I);
    assert(7I / 2I == 3I);
}
