fun main() {
  let x = X::one(1);

  assert(x.a == 1);
  assert(x.b == 0);
  assert(x.c == 0);

  let x = X::two(1, 2);

  assert(x.a == 1);
  assert(x.b == 2);
  assert(x.c == 0);

  let x = X(1, 2, 3);

  assert(x.a == 1);
  assert(x.b == 2);
  assert(x.c == 3);
}

class X(let a: Int, let b: Int, let c: Int) {
  @static fun one(a: Int) -> X = X(a, 0, 0);
  @static fun two(a: Int, b: Int) -> X = X(a, b, 0);
}
