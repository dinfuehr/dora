fun main() {
  let x = X::one(1);

  assert(x.a == 1);
  assert(x.b == 0);
  assert(x.c == 0);

  let x = X::two(1, 2);

  assert(x.a == 1);
  assert(x.b == 2);
  assert(x.c == 0);

  let x = X(1, 2, 3);

  assert(x.a == 1);
  assert(x.b == 2);
  assert(x.c == 3);
}

class X(let a: Int32, let b: Int32, let c: Int32) {}

module X {
  fun one(a: Int32): X = X(a, 0, 0);
  fun two(a: Int32, b: Int32): X = X(a, b, 0);
}
