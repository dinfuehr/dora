fun main() {
    let x = Array[Char]::empty();
    assert(x.size() == 0L);

    let x = Array[Char]::fill(10L, '\0');
    assert(x(0L) == '\0');
    assert(x(9L) == '\0');

    x(0L) = 'a';
    x(1L) = 'b';
    x(2L) = 'c';
    x(3L) = 'd';
    x(9L) = 'z';

    println(x(3L).toString());

    assert(x(3L) == 'd');
    assert(x(4L) == '\0');
    assert(x(8L) == '\0');
    assert(x(9L) == 'z');
}