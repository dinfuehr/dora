fun main(): Unit {
    let x = Array[Char]::new();
    assert(x.size() == 0i64);

    let x = Array[Char]::fill(10i64, '\0');
    assert(x(0i64) == '\0');
    assert(x(9i64) == '\0');

    x(0i64) = 'a';
    x(1i64) = 'b';
    x(2i64) = 'c';
    x(3i64) = 'd';
    x(9i64) = 'z';

    println(x(3i64).toString());

    assert(x(3i64) == 'd');
    assert(x(4i64) == '\0');
    assert(x(8i64) == '\0');
    assert(x(9i64) == 'z');
}