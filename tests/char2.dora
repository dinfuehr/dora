//= cannon

fun main() {
    let x = arrayEmpty[Char]();
    assert(x.length() == 0);

    let x = arrayFill[Char](10, '\0');
    assert(x(0) == '\0');
    assert(x(9) == '\0');

    x(0) = 'a';
    x(1) = 'b';
    x(2) = 'c';
    x(3) = 'd';
    x(9) = 'z';

    println(x(3).toString());

    assert(x(3) == 'd');
    assert(x(4) == '\0');
    assert(x(8) == '\0');
    assert(x(9) == 'z');
}