fun main() {
  let x = defaultValue[Bool]();
  assert(x == false);

  let x = defaultValue[UInt8]();
  assert(x == 0Y);

  let x = defaultValue[Char]();
  assert(x == '\0');

  let x = defaultValue[Int32]();
  assert(x == 0);

  let x = defaultValue[Int64]();
  assert(x == 0L);

  let x = defaultValue[Float32]();
  assert(x == 0.0F);

  let x = defaultValue[Float64]();
  assert(x == 0.0);

  let x = defaultValue[String]();
  assert(x === nil);

  let x = defaultValue[Array[Int32]]();
  assert(x === nil);

  let x = defaultValue[Array[Float32] ]();
  assert(x === nil);
}
