//= stdout file
use std::string::Stringable;

fn main() {
    let mut i = 1i32;

    while i <= 100i32 {
        if i % 15i32 == 0i32 {
            println("FizzBuzz");

        } else if i % 5i32 == 0i32 {
            println("Buzz");

        } else if i % 3i32 == 0i32 {
            println("Fizz");
        } else {
            println(i.toString());
        }

        i = i + 1i32;
    }
}
