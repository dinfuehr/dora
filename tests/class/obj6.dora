fn main() {
  let f = Foo(12i32);
  let mut a1 = f.a1();
  let a2 = f.a2();
  assert(a1 == a2);

  f.setA(101i32);
  a1 = f.a1();
  assert(101i32 == a1);
}

class Foo(a: Int32)

impl Foo {
  fn a1(): Int32 { return self.a; }
  fn a2(): Int32 { return self.a; }

  fn setA(a: Int32) { self.a = a; }
}
