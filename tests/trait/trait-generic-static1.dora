//= cannon

fun main() {
    assert(f[Bar]() == 1I);
    assert(f[Baz]() == 2I);
}

trait X {
    @static fun foo() -> Int32;
}

fun f[T: X]() -> Int32 {
    return T::foo();
}

class Bar

impl X for Bar {
    @static fun foo() -> Int32 {
        return 1I;
    }
}

class Baz

impl X for Baz {
    @static fun foo() -> Int32 {
        return 2I;
    }
}