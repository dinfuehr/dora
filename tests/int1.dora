fun main() {
    let x = 1.unaryPlus();
    assert(x == +1);

    let x = 1.unaryMinus();
    assert(x == -1);

    let x = 1.plus(2);
    assert(x == 3);

    let x = 1 + 2;
    assert(x == 3);

    let x = 5.minus(2);
    assert(x == 3);

    let x = 5 - 2;
    assert(x == 3);

    let x = 3.times(4);
    assert(x == 12);

    let x = 3 * 4;
    assert(x == 12);

    let x = 9.div(8);
    assert(x == 1);

    let x = 9 / 8;
    assert(x == 1);

    let x = 10.mod(8);
    assert(x == 2);

    let x = 10 % 8;
    assert(x == 2);

    let x = 1.bitwiseOr(2);
    assert(x == 3);

    let x = 1 | 2;
    assert(x == 3);

    let x = 5.bitwiseAnd(6);
    assert(x == 4);

    let x = 5 & 6;
    assert(x == 4);

    let x = 1.bitwiseXor(3);
    assert(x == 2);

    let x = 1 ^ 3;
    assert(x == 2);

    let x = 1.not();
    assert(x == -2);

    let x = !1;
    assert(x == -2);

    let x = 1.shiftLeft(1);
    assert(x == 2);

    let x = 1 << 1;
    assert(x == 2);

    let x = 6.shiftRightSigned(1);
    assert(x == 3);

    let x = 6 >> 1;
    assert(x == 3);

    let x = 12.shiftRight(2);
    assert(x == 3);

    let x = 12 >> 2;
    assert(x == 3);

    let x = (-12).rotateLeft(1);
    assert(x == -23);

    let x = (-12).rotateRight(1);
    assert(x == 2147483642);

    let x = (-12).toStringBinary();
    assert(x == "11111111111111111111111111110100");

    assert(2 != 3);
    assert(5 == 5);
    assert(2 > 1);
    assert(2 >= 1);
    assert(3 < 4);
    assert(3 <= 4);
}