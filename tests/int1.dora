fun main(): Unit {
    let x = 1i32.unaryPlus();
    assert(x == +1i32);

    let x = 1i32.unaryMinus();
    assert(x == -1i32);

    let x = 1i32.plus(2i32);
    assert(x == 3i32);

    let x = 1i32 + 2i32;
    assert(x == 3i32);

    let x = 5i32.minus(2i32);
    assert(x == 3i32);

    let x = 5i32 - 2i32;
    assert(x == 3i32);

    let x = 3i32.times(4i32);
    assert(x == 12i32);

    let x = 3i32 * 4i32;
    assert(x == 12i32);

    let x = 9i32.div(8i32);
    assert(x == 1i32);

    let x = 9i32 / 8i32;
    assert(x == 1i32);

    let x = 10i32.remainder(8i32);
    assert(x == 2i32);

    let x = 1i32.bitwiseOr(2i32);
    assert(x == 3i32);

    let x = 1i32 | 2i32;
    assert(x == 3i32);

    let x = 5i32.bitwiseAnd(6i32);
    assert(x == 4i32);

    let x = 5i32 & 6i32;
    assert(x == 4i32);

    let x = 1i32.bitwiseXor(3i32);
    assert(x == 2i32);

    let x = 1i32 ^ 3i32;
    assert(x == 2i32);

    let x = 1i32.not();
    assert(x == -2i32);

    assert(2i32 != 3i32);
    assert(5i32 == 5i32);
    assert(2i32 > 1i32);
    assert(2i32 >= 1i32);
    assert(3i32 < 4i32);
    assert(3i32 <= 4i32);
}
