fn main() {
  let a = nested();
  std::forceCollect();

  assert(a.next.getOrPanic().next.getOrPanic().value == 3i32);
}

fn nested(): Foo {
  let x = Foo(3i32, None[Foo]);
  let y = Foo(2i32, Some[Foo](x));
  let z = Foo(1i32, Some[Foo](y));

  return z;
}

class Foo(value: Int32, next: Option[Foo])
