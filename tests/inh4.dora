fun main() {
  let b = B();
  assert(b.printBase() == 2);
  assert(b.printSpecial() == 1);

  let c = C();
  assert(c.printBase() == 2);
  assert(c.printSpecial() == 1);
}

@open @abstract class A {
 @abstract fun printSpecial(): Int32;
 fun printBase(): Int32 {
   return 2;
 }
}

@open class B extends A {
 @override fun printSpecial(): Int32 {
   return 1;
 }
}

class C extends B {

}
