//= vm-args "--gc=copy --disable-tlab --gc-stress"

struct Bar(first: Foo, second: Foo)
class Foo(value: Int32)

fn main() {
    let tpl: Bar = if false {
        Bar(createFoo(), createFoo())
    } else {
        std::forceCollect();
        Bar(createFoo(), createFoo())
    };
}

fn createFoo(): Foo { Foo(1i32) }
