struct Foo(f1: Int32, f2: Bool)

fun f1(x: Foo, control: Int32): Int32 {
    assert(control == 4711I);
    x.f1
}

fun f2(x: Foo, control: Int32): Bool {
    assert(control == 90210I);
    x.f2
}

fun main() {
    assert(f1(Foo(10I, false), 4711I) == 10I);
    assert(!f2(Foo(10I, false), 90210I));
}
