//= vm-args "--gc=copy --disable-tlab --gc-stress"

class TupleClass {
    var value: Pair = Pair(Foo(0I), 0L);
}

struct Pair(first: Foo, second: Int64)
class Foo(let value: Int32)

fun main() {
    let tpl = TupleClass();
    std::forceCollect();
    assert(tpl.value.first.value == 0I);
    assert(tpl.value.second == 0L);
    tpl.value = Pair(Foo(4711I), 4712L);
    std::forceCollect();
    assert(tpl.value.first.value == 4711I);
    assert(tpl.value.second == 4712L);
}
