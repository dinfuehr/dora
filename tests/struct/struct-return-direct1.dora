//= vm-args "--gc=copy --disable-tlab --gc-stress"

fn main() {
    let x = Bar(42i32).tpl1();
    std::forceCollect();
    assert(x.1 .0 == 17i32);
} 

struct Pair(Int32, Foo)
class Bar &(Int32)

impl Bar {
    fn tpl1(): Pair {
        assert(self.0 == 42i32);
        Pair(1i32, Foo(17i32))
    }
}

class Foo &(Int32)
