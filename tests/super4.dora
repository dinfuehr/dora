fun main() {
  let c = C();

  assert(c.foo(1) == 3);
  assert(c.foo(2) == 5);
}

@open class A {
  @open fun foo(a: Int32): Int32 {
    return a;
  }
}

@open class B: A {
  @open @override fun foo(a: Int32): Int32 {
    return super.foo(a) * 2;
  }
}

class C: B {
  @override fun foo(a: Int32): Int32 {
    return super.foo(a) + 1;
  }
}
