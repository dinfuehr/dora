fun main() {
    let bar = Bar::<int>(11);
    assert(bar.x.fst == 11);
    assert(bar.x.snd == 10);

    let bar = Bar::<String>("hello");
    assert(bar.x.fst == "hello");
    assert(bar.x.snd == 10);
}

class Bar<T>(val: T) {
    let x: Pair<T, int> = Pair::<T, int>(val, 10);
}

class Pair<A, B>(let fst: A, let snd: B)
