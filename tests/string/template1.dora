fun main(): Unit {
    let x = "a${1}b${2}c${3}d";
    assert(x == "a1b2c3d");

    let x = "a${Foo(1i32)}b${Foo(2i32)}c${Foo(3i32)}d";
    assert(x == "a1b2c3d");

    let x = "a${1.1}b${2.3f32}c${4.3}d";
    assert(x == "a1.1b2.3c4.3d");

    let val = 4i32;
    let x = "a${val*3i32}b${1i32+val}c${val%2i32}d${val.toString()}e";
    assert(x == "a12b5c0d4e");
}

class Foo(i: Int32)

impl std::Stringable for Foo {
    fun toString(): String {
        std::forceCollect();
        return self.i.toString();
    }
}