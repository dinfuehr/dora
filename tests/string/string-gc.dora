//= stdout "abxy\n"

fun main(): Unit {
    let x = Array[String]::new();
    assert(x.size() == 0i64);

    let emptyString = "".clone();
    let x = Array[String]::fill(10i64, emptyString);
    assert(x.size() == 10i64);

    x(0i64) = "a" + "b";
    x(1i64) = "x" + "y";

    std::forceCollect();

    print(x(0i64));
    println(x(1i64));

    assert(x(0i64) == "ab");
    assert(x(1i64) == "xy");
    assert(x(2i64) === emptyString);
    assert(x(9i64) === emptyString);
}