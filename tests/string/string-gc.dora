//= stdout "abxy\n"

fun main() {
    let x = arrayEmpty[String]();
    assert(x.length() == 0);

    let x = arrayFill[String](10, nil);
    assert(x.length() == 10);

    x(0) = "a" + "b";
    x(1) = "x" + "y";

    forceCollect();

    print(x(0));
    println(x(1));

    assert(x(0) == "ab");
    assert(x(1) == "xy");
    assert(x(2) === nil);
    assert(x(9) === nil);
}