//= stdout "abxy\n"
//= cannon

fun main() {
    let x = arrayEmpty[String]();
    assert(x.length() == 0L);

    let x = arrayFill[String](10L, nil);
    assert(x.length() == 10L);

    x(0L) = "a" + "b";
    x(1L) = "x" + "y";

    forceCollect();

    print(x(0L));
    println(x(1L));

    assert(x(0L) == "ab");
    assert(x(1L) == "xy");
    assert(x(2L) === nil);
    assert(x(9L) === nil);
}