fun main() {
    let ba = Array[Int64]::zero(0L);
    assert(ba.size() == 0L);

    let ba = Array[Int64]::fill(10L, 2L);
    assert(ba.size() == 10L);

    ba(0L) = 10L;
    ba(1L) = 11L;
    ba(2L) = 12L;

    assert(10L == ba(0L));
    assert(11L == ba(1L));
    assert(12L == ba(2L));
    assert(2L == ba(9L));
}
