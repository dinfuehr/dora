//= vm-args "--gc=copy"

class MyThread extends std::Thread {
    @override fun run() {}
}

fun main() {
    var i = 0I;

    while i < 10_000I {
        let thread = MyThread();
        thread.start();

        if i % 1_000I == 0I {
            std::forceCollect();
        }

        i = i + 1I;
    }
}
