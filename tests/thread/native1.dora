import std::Thread;

class MyThread() extends Thread {
    @override fun run() {
        nativeCalls();
    }
}

fun main() {
    let thread = MyThread();
    thread.start();

    nativeCalls();
}

fun nativeCalls() {
    var i = 0;

    while i < 100_000 {
        std::sleep(0);
        i = i + 1;
    }
}