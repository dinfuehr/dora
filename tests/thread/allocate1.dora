//= output "done\ndone\n"
//= vm-args "--gc-young-size=64M --max-heap-size=128M"

class MyThread() : Thread {
    @override fun run() {
        allocator();
        println("done");
    }
}

fun main() {
    let thread = MyThread();
    thread.start();

    allocator();
    println("done");
}

fun allocator() {
    var i = 0;

    while i < 10_000 {
        Foo();
        i = i + 1;
    }
}

class Foo