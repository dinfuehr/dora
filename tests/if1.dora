//= vm-args "--gc=copy"

fun main() {
    assert(f(true) == 1I);
    assert(f(false) == 2I);

    assert(g(true) == 1I);
    assert(g(false) == 2I);

    assert(h(true, 7I) == "t7end");
    assert(h(false, 12I) == "f12end");

    assert(i(true) == "1");
    assert(i(false) == "not one");
}

fun f(x: Bool): Int32 = if x { 1I } else { 2I };

fun g(x: Bool): Int32 {
    let ret = if x { 1I } else { 2I };

    ret
}

fun h(x: Bool, y: Int32): String {
    (if x { "t${y}" } else { "f${y}" }) + { std::forceCollect(); "end" }
}

fun i(x: Bool): String {
  let i = if x { 1I } else { return "not one"; };
  i.toString()
}